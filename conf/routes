# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)


#Jack Api
GET   /api/jack/fetch              controllers.JackController.fetchTubeLine
GET    /api/jack/:id               controllers.JackController.find(id:String)
POST   /api/jack                   controllers.JackController.save
DELETE /api/jack/:id               controllers.JackController.delete(id:String)

